@using KlamathIrrigationDistrict.DataLayer.DataModels
@*@model KlamathIrrigationDistrict.Models.StructureListModel*@
@model DitchRiderRequests
@{
    ViewBag.Title = "KID D.R.: Call Requests";
}
<head>
    <title>KID: Ditch Rider</title>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Styles/DitchRiders.css" />
    <script src="~/Scripts/KIDDitchRidersJS.js"></script>
</head>
<!--List of Rides class=dropdownRide-->
<header id="Header">
    <div id="RideCenter">
        <li class="dropdown">
            <a class="./KIDDitchRider4HTML.html">Ride 5</a>
        </li>
    </div>
    <!--Log in Name, Customers, Requests, Canals, LogOUt id=Menu-->
    <ul id="Menu">
        <li><label class="FirstLast">Dillon.Nowak</label></li>
        <li><a href="./KIDDitchRiderRide4CustomersHTML.html">Customers</a></li>
        <li class="dropdown">
            <a class="RequestDropDown" href="/DitchRiders/Index5">Requests</a>
            <!--Requests drop down menu class=dropdownRequest-->
            <div class="dropdownRequest" style="overflow-x: auto;">
                <a href="./KIDDitchRiderRide4AppendingWaitlistHTML.html">Appending/Waitlist</a>
                <a href="/DitchRiders/AddRequest" id="active">Call Requests</a>
            </div>
        </li>
        <li class="dropdown">
            <a class="RequestDropDown" href="./KIDDitchRiderRide4CanalsHTML.html">Canals</a>
            <!--List of Cananls on Ride class=dropdownRequest-->
            <div class="dropdownRequest">
                <a href="./KIDDitchRiderRide4CanalsAHTML.html">A</a>
                <a href="./KIDDitchRiderRide4CanalsCHTML.html">C</a>
                <a href="./KIDDitchRiderRide4CanalsC4HTML.html">C4</a>
                <a href="./KIDDitchRiderRide4CanalsC4EHTML.html">C4E</a>
                <a href="./KIDDitchRiderRide4CanalsC4E1HTML.html">C4E1</a>
                <a href="./KIDDitchRiderRide4CanalsC4E1AHTML.html">C4E1A</a>
                <a href="./KIDDitchRiderRide4CanalsC4JHTML.html">C3J</a>
                <a href="./KIDDitchRiderRide4CanalsC4KHTML.html">C4K</a>
                <a href="./KIDDitchRiderRide4CanalsC4LHTML.html">C4L</a>
                <a href="./KIDDitchRiderRide4CanalsC4MHTML.html">C4M</a>
                <a href="./KIDDitchRiderRide4CanalsC4NHTML.html">C4N</a>
                <a href="./KIDDitchRiderRide4CanalsC4QHTML.html">C4Q</a>
                <a href="./KIDDitchRiderRide4CanalsC4RHTML.html">C4R</a>
                <a href="./KIDDitchRiderRide4CanalsC4SHTML.html">C4S</a>
            </div>
        </li>
        <a href="~/Account/Login"><button class="LogOutBtn">Log Out</button></a>
    </ul>
</header>
<body>
    <div style="overflow-x: auto;">
        <!--Alphabet List for ActiveRequests class=ActiveRequests-->
        <table class="ActiveRequests">
            <caption>Add Request(on)</caption>
            <tr>
                <th>@Html.Label("Request Date")</th>
                <th>@Html.Label("Customer ID")</th>
                <th>@Html.Label("Customer Name")</th>
                <th>@Html.Label("Turn Out")</th>
                <th>@Html.Label("CFS Requested")</th>
                <th colspan="2">@Html.Label("Customer Comments")</th>
            </tr>
            <tr>
                @Html.HiddenFor(m => m.RequestID)
                <td>@Html.TextBoxFor(m => m.CustomerDate1, new { @type = "date", @placeholder = "02/02/2018" })</td>
                <td>@Html.TextBoxFor(m => m.CustomerID, new { @type = "number", @Value = "760" })</td>
                <td>@Html.TextBoxFor(m => m.CustomerName, new { @Value = "Kliewer Ryan & Laurinda" })</td>
                <td>@Html.TextBoxFor(m => m.Structure)</td>
                @*<td><input list="HorsleyStructures"</td>*@
                <td>@Html.TextBoxFor(m => m.CustomerCFS1, new { @type = "number" })</td>
                <td>@Html.TextBoxFor(m => m.CustomerComments1, new { @Value = "Made by Dillon Nowak" })</td>
                <td><button type="submit">Add</button></td>
            </tr>
        </table>
        @Html.Partial("_AddRequest5Off")
    </div>
</body>
<datalist id="HorsleyStructures">
    <option value="T4603"></option>
    <option value="T4604"></option>
    <option value="T4605"></option>
    <option value="T4607"></option>
    <option value="P4609"></option>
    <option value="T4610"></option>
</datalist>