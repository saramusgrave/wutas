<body>
    <a href="Index4" class="previous">&laquo; Previous</a><br />
    Update Request
    @using (Html.BeginForm())
    {
        @Html.HiddenFor(s => s.RequestID)
        @Html.HiddenFor(s => s.KIDStaffID2)

        <div>
            @Html.Label("Customer Name: ")
            @Html.TextBoxFor(s => s.CustomerName, new { @disabled = "diabled", @class = "CustomerName" })
        </div>
        <div>
            @Html.Label("Time Stamp: ")
            @Html.TextBoxFor(s => s.TimeStampStaff2, new { @type = "datetime", @disabled = "disabled" })
        </div>
        <div>
            @Html.Label("Date: ")
            @Html.TextBoxFor(s => s.StaffDate2, new { @type = "date" })
        </div>
        <div>
            @Html.Label("Status: ")
            @Html.TextBoxFor(s => s.RequestStatus2)
            @*<input list="RequestStatus" />*@
        </div>
        <div>
            @Html.Label("CFS: ")
            @Html.TextBoxFor(s => s.StaffCFS2, new { @type = "number" })
        </div>
        <div>
            @Html.Label("Comments: ")
            @Html.TextAreaFor(s => s.StaffComments2)
        </div>
        <div>
            <button type="submit">Submit</button>
        </div>
    }
</body>
<datalist id="RequestStatus">
    <option value="Confirm">Confirm</option>
    <option value="Reject">Reject</option>
    <option value="Waitlist">Waitlist</option>
</datalist>