@using KlamathIrrigationDistrict.DataLayer.DataModels
@model Customers
@{ 
    ViewBag.Title = "KID Staff: Edit Customers";
}
<head>
<title>KID: Office Staff</title>
<meta name="viewport" content="width=device-width" />
<link rel="stylesheet" href="~/Styles/OfficeStaff.css" />
<script src="~/Scripts/KIDDitchRidersJS.js"></script>
</head>
<!--Log in Name, Customers, Ditch Riders, Log Out id=OfficeStaffMenu-->
<header id="OfficeStaffMenu">
    <ul>
        <li><label class="FirstLast">Linda.Seater</label></li>
        <li><a href="/Customers/Index" id="active">Customers</a></li>
        <li><a href="Index">Staff</a></li>
        <li><a href="/MapTaxLot/Index">MapTaxLots</a></li>
        <li><a href="~/Account/Login" class="LogOutBtn">Log Out</a></li>
    </ul>
</header>
<body>
    <a href="/Customers/Index" class="previous">&laquo; Previous</a>
    @using (Html.BeginForm())
    {
        <div>
            CustomerID:@Html.Editor("CustomerID")
        </div>
        <div>
            TrackingID: @Html.Editor("TrackingID")
        </div>
        <div>
            Name: @Html.Editor("Name")
        </div>
        <div>
            Address 1: @Html.Editor("Address1")
        </div>
        <div>
            Address 2: @Html.Editor("Address2")
        </div>
        <div>
            City: @Html.Editor("City")
        </div>
        <div>
            State: @Html.Editor("State")
        </div>
        <div>
            Zip: @Html.Editor("Zip")
        </div>
        <div>
            Total Allotment: @Html.Editor("TotalAllotment")
        </div>
        <div>
            <button type="submit">Submit</button>
        </div>
    }
</body>
